

do 
  let plies = (slow 2 $ "<3 4 5 7>") :: Pattern (Ratio Integer)
      denom = 4
      period = 0.5
      hatPlyFunc = (# pad11)
      kickPlyFunc = (# gain 0.8)
      synthUp = (range 0 127 $ slow 30 sine)
      synthDown = 0
      synthMute = id 
      hatMute = id 
      drumsMute = id
      kickFill = g
  d1 
    $ rising plies denom period kickPlyFunc hatPlyFunc synthMute hatMute drumsMute synthUp synthDown kickFill


hush